{
    "$schema": "https://json.schemastore.org/package.json",
    "name": "@xenous/client",
    "version": "0.1.0",
    "private": true,
    "type": "module",
    "scripts": {
        "build": "pnpm with-env next build",
        "clean": "git clean -xdf .next .turbo node_modules",
        "dev": "pnpm with-env next dev",
        "format": "prettier --check . --ignore-path ../../.gitignore --ignore-path .prettierignore",
        "lint": "eslint",
        "start": "pnpm with-env next start",
        "typecheck": "tsc --noEmit",
        "with-env": "dotenv -e ./.env.local -e ../../.env -c --",
        "knip": "knip --config ../../knip.json"
    },
    "dependencies": {
        "@t3-oss/env-nextjs": "^0.9.2",
        "@tanstack/react-query": "^5.29.0",
        "@tanstack/react-query-devtools": "^5.29.2",
        "@trpc/client": "11.0.0-next.320",
        "@trpc/react-query": "11.0.0-next.320",
        "@trpc/server": "11.0.0-next.320",
        "@xenous/api": "workspace:*",
        "@xenous/auth": "workspace:*",
        "@xenous/db": "workspace:*",
        "@xenous/http": "workspace:*",
        "@xenous/logger": "workspace:*",
        "@xenous/schema": "workspace:*",
        "@xenous/ui": "workspace:*",
        "date-fns": "^3.6.0",
        "next": "^14.1.4",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "react-hook-form": "^7.51.1",
        "sharp": "^0.33.3",
        "superjson": "2.2.1",
        "zod": "^3.22.4"
    },
    "devDependencies": {
        "@svgr/webpack": "^8.1.0",
        "@types/node": "^20.12.5",
        "@types/react": "^18.2.74",
        "@types/react-dom": "^18.2.24",
        "@xenous/eslint-config": "workspace:*",
        "@xenous/prettier-config": "workspace:*",
        "@xenous/tailwind-config": "workspace:*",
        "@xenous/tsconfig": "workspace:*",
        "autoprefixer": "^10.4.19",
        "dotenv-cli": "^7.4.1",
        "eslint": "^9.0.0",
        "jiti": "^1.21.0",
        "prettier": "^3.2.5",
        "rwdot": "^0.0.5",
        "sass": "^1.76.0",
        "tailwindcss": "^3.4.1",
        "typescript": "^5.4.4"
    },
    "prettier": "@xenous/prettier-config"
}
