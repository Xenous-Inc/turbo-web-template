{
    "$schema": "https://raw.githubusercontent.com/expo/eas-cli/main/packages/eas-json/schema/eas.schema.json",
    "cli": {
        "version": ">= 4.1.2"
    },
    "build": {
        "base": {
            "node": "20.16.0",
            "pnpm": "9.7.0",
            "ios": {
                "resourceClass": "m-medium"
            }
        },
        "development": {
            "extends": "base",
            "developmentClient": true,
            "distribution": "internal"
        },
        "preview": {
            "extends": "base",
            "distribution": "internal",
            "ios": {
                "simulator": true
            }
        },
        "production": {
            "extends": "base"
        }
    },
    "submit": {
        "production": {}
    }
}
